<p id="notice"><%= notice %></p>

<h1>Admin Dumpsters</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_dumpsters.each do |admin_dumpster| %>
      <tr>
        <td><%= admin_dumpster.post %></td>
        <td><%= admin_dumpster.admin %></td>
        <td><%= link_to 'Show', admin_dumpster %></td>
        <td><%= link_to 'Edit', edit_admin_dumpster_path(admin_dumpster) %></td>
        <td><%= link_to 'Destroy', admin_dumpster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Dumpster', new_admin_dumpster_path %>
