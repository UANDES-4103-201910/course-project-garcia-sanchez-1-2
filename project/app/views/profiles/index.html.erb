<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Photo</th>
      <th>Biography</th>
      <th>Geofence</th>
      <th>Country</th>
      <th>City</th>
      <th>Registred user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users = User.all %>
    <% @users.each do |u| %>
      <tr>
        <td><%= u.username %></td>
        
        <td><%= link_to 'Show', u %></td>
        <td><%= link_to 'Edit', edit_user_path(u) %></td>
        <td><%= link_to 'Destroy', u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
