<!DOCTYPE html>
<html>

  <head>
    <title>Project Garcia Sanchez</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% flash.each do |type, message| %>
    <div class="alert <%= bootstrap_class_for(type) %> alert-dismissible" role="error">
      
      <%= message %>
    </div>
    <% end %>

    <%= yield %>
  </body>
</html>
